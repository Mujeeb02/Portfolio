{
  "extends": [
    "eslint:recommended",
    "next/core-web-vitals"
  ],
  "rules": {
    "react/prop-types": "off",
    "no-unused-vars": "warn",
    "@next/next/no-page-custom-font": "off",
    "react/no-unescaped-entities": ["error", {
      "forbid": [">", "}", "]", "\"", "'", "}"]
    }]
  }
}
